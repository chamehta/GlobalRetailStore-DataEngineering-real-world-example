year = int(input("Enter year: "))
if year == 1:
  print ("Freshman")
else:
  if year == 2:
     print ("Sophomore")
  else:
     if year == 3:
        print ("Junior")
     else:
        if year == 4:
           print ("Senior")

#
year=int(input("enter the year:"))
if year == 1:
    print("Freshman")
elif year==2:
    print("Sophomore")
elif year==3:
    print("Junior")
elif year==4:
    print("Senior")
else:
    print("Not a valid year")